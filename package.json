{
  "name": "iron-log",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:test": "cross-env NODE_ENV=test next dev -p 7357",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "pw": "npm run db:test && playwright test",
