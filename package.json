{
  "name": "iron-log",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:test": "cross-env NODE_ENV=test next dev -p 7357",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "pw": "npm run db:test && playwright test",
    "cg:phone": "playwright codegen --device \"iPhone 15 Pro Max\" localhost:7357",
    "cg:chrome": "playwright codegen --viewport-size \"1000,800\" localhost:7357",
    "test": "vitest --ui --open=false --hideSkippedTests",
    "test:once": "vitest run --allowOnly=false",
    "test:start": "vitest --ui --open=false --standalone",
    "test:view": "vite preview --outDir vitest/html",
    "db:dev": "vite-node scripts/mongoSeedDev.ts",
    "db:test": "vite-node scripts/mongoResetTest.ts",
    "knip": "knip"
  },
