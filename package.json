{
  "name": "iron-log",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:test": "NODE_ENV=test next dev -p 7357",
    "build": "next build",
    "start": "next start",
    "start:test": "next start -p 7357",
    "lint": "biome check --reporter=summary",
    "lint:diff": "biome check --reporter=summary --changed --no-errors-on-unmatched",
    "lint:fix": "biome check --fix --unsafe --no-errors-on-unmatched",
    "prepare": "husky",
    "pw": "bun db:test && playwright test",
    "cg:phone": "playwright codegen --device \"iPhone 15 Pro Max\" localhost:7357",
    "cg:chrome": "playwright codegen --viewport-size \"1000,800\" localhost:7357",
    "vi": "vitest --ui --open=false --hideSkippedTests",
    "vi:once": "vitest run --allowOnly=false",
    "vi:start": "vitest --ui --open=false --standalone",
    "vi:view": "vite preview --outDir vitest/html",
    "db:dev": "bun scripts/mongoSeedDev.ts",
    "db:test": "bun scripts/mongoResetTest.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.11.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-date-pickers": "^8.21.0",
    "@tanstack/react-query": "^5.90.12",
    "@vercel/analytics": "^1.6.1",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "http-status-codes": "^2.3.0",
    "mongodb": "^7.0.0",
    "next": "^16.0.7",
    "next-auth": "^4.24.13",
    "notistack": "^3.0.2",
    "nuqs": "^2.8.2",
    "react": "^19.2.1",
    "react-fast-compare": "^3.2.2",
    "react-imask": "^7.6.1",
    "react-resize-detector": "^12.3.0",
    "react-transition-group": "^4.4.5",
    "recharts": "^3.5.1",
    "swiper": "^12.0.3",
    "use-local-storage-state": "^19.5.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.7",
    "knip": "^5.71.0",
    "lint-staged": "^16.2.7",
    "mongodb-memory-server": "^10.4.1",
    "prettier": "^3.7.4",
    "react-dom": "^19.2.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
