    "test:start": "vitest --ui --open=false --standalone",
    "test:view": "vite preview --outDir vitest/html",
    "db:dev": "vite-node scripts/mongoSeedDev.ts",
    "db:test": "vite-node scripts/mongoResetTest.ts",
    "knip": "knip"
  },
